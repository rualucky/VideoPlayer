var myobject=function(id,name){
	this.id=id;
	this.name=name;
};

myobject.prototype={
	id:0,
	name:null,
	show:function(){
		console.log(this.name);
	},
	hide:function(){}
};


var obj=new myobject(5,"namme");
obj.show();
