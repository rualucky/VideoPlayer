video::-webkit-media-controls {
    display:none !important;
}
.mep-html-box *::-webkit-media-controls-enclosure ,.mep-html-box *::-webkit-media-controls,.mep-html-box  video::media-controls{
  display:none !important;
}
.mep-html-box{
    position:relative;
    -moz-user-select: none;
  -ms-user-select: none;
  -webkit-user-select: none;
  -moz-box-sizing: border-box;
  width:100%;
  height:100%;
  position:absolute !important;
  padding-bottom: 30px !important;
  overflow: hidden;
}
.mep-html-box video.mep-video-stage{
    background: #000;
    height:100%;
    width:100%;
}
.mep-html-box svg{
    fill:currentColor !important;
}
.mep-html-box .mep-ctr{
    background: #333;
    height: 30px;
    bottom:0px;
    width:100%;
    position: absolute;
    z-index: 2147483647;
    left:0px;
    overflow: hidden;
}
/*
.mep-html-box .mep-ctr:fullscreen{
    left:0px;
    bottom:0px;
}*/
.mep-html-box .mep-btn-fullscreen, 
.mep-html-box .mep-comp-timeduration,
.mep-html-box .mep-btn-quality,
.mep-html-box .mep-btn-meicon{
    float:right;
}
.mep-html-box .mep-btn-play,
.mep-html-box .mep-btn-volume,
.mep-html-box .mep-btn-time-line{
    float:left;
}

.mep-html-box .mep-btn-play{width:15px;}
.mep-html-box .mep-btn-play .mep-playing{display:none;}
.mep-html-box .mep-btn-play .mep-pause{display:block;}
.mep-html-box .mep-btn-play.mep-status-active .mep-playing{display:block;}
.mep-html-box .mep-btn-play.mep-status-active .mep-pause{display:none;}
.mep-html-box .mep-btn-volume{width:22px;}
.mep-html-box .mep-btn-volume:hover{width:78px;}
.mep-html-box .mep-btn-volume .mep-volume-panel{
    display:none;
}
.mep-html-box .mep-btn-volume:hover .mep-volume-panel{
    display:block;
}
.mep-html-box .mep-btn-volume .mep-mute{display:none;float:left;}
.mep-html-box .mep-btn-volume .mep-unmute{display:block;float:left;}
.mep-html-box .mep-btn-volume.mep-status-mute .mep-mute{display:block;}
.mep-html-box .mep-btn-volume.mep-status-mute .mep-unmute{display:none;}
.mep-volume-panel {
  -ms-touch-action: none;
  display: block;
  width: 51px;
  overflow: hidden;
  -moz-transition: width .5s ease-out .2s;
  -webkit-transition: width .5s ease-out .2s;
  transition: width .5s ease-out .2s;
  float: left;
  
}
.mep-volume-panel:focus{outline: none;}
.mep-volume-slider {
  position: relative;
  display: block;
  height: 23px;
  width: 51px;
  float: left;
  margin: 0;
  padding: 0;
  cursor: pointer;
  overflow: hidden;
}
.mep-volume-slider-foreground {
  position: absolute;
  bottom: 6.5px;
  height: 14px;
  width: 4px;
  background: #aaa;
}
.mep-volume-slider-foreground:before {
  position: absolute;
  top: 5px;
  left: -55px;
  display: block;
  width: 55px;
  height: 4px;
  background: #b91f1f;
  content: '';
}
.mep-volume-slider-foreground:after {
  position: absolute;
  top: 5px;
  display: block;
  width: 55px;
  height: 4px;
  content: '';
  background: #777;
  left: 4px;
}
.mep-html-box .mep-btn-time-line{}
.mep-html-box .mep-btn-fullscreen{width:15px;} 
.mep-html-box .mep-btn-quality{width:15px;}
.mep-html-box .mep-btn-meicon{width:15px;}

.mep-html-box .mep-btn-play,
.mep-html-box .mep-btn-volume,
.mep-html-box .mep-btn-time-line,
.mep-html-box .mep-btn-fullscreen, 
.mep-html-box .mep-btn-quality,
.mep-html-box .mep-btn-meicon{
    color: #aaa;
    height: 100%;
    padding: 6px 7px;
    cursor: pointer;
    display:inline-block;
}
.mep-html-box .mep-btn-play:hover,
.mep-html-box .mep-btn-volume:hover,
.mep-html-box .mep-btn-time-line:hover,
.mep-html-box .mep-btn-fullscreen:hover, 
.mep-html-box .mep-comp-timeduration:hover,
.mep-html-box .mep-btn-quality:hover,
.mep-html-box .mep-btn-meicon:hover{
    color:#fff;
}
.mep-html-box .mep-comp-timeduration{
    top:0;
    position: absolute;
    height: 3px;
    width:100%;
    background: #aaa;
}
.mep-html-box .mep-comp-timeduration{
    -webkit-transition: height 0.5s, top 0.5s; /* For Safari 3.1 to 6.0 */
    transition: height 0.5s, top 0.5s;
   -webkit-transition-delay: 3s; /* Safari */
    transition-delay: 3s;
}
.mep-html-box:hover .mep-comp-timeduration{
    height: 6px;
    top:-3px;
    -webkit-transition: height 0.5s, top 0.5s; /* For Safari 3.1 to 6.0 */
    transition: height 0.5s, top 0.5s;
}
.mep-html-box .mep-comp-timeduration .mep-time-duration-loaded{
    height: 100%;
    display: block;
    background: #3EA9F5;
}
.mep-html-box .mep-comp-timeduration{
    
}
.mep-html-box .mep-overlay-player{
    background: url('./images/tbg.png');
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    bottom: 0;
}
